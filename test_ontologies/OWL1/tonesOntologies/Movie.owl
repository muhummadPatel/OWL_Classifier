        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.owl-ontologies.com/Movie.owl#hasExtraGenre"/>
                        <owl:someValuesFrom rdf:resource="http://www.owl-ontologies.com/Movie.owl#ExtraParody"/>
