                        <owl:onProperty rdf:resource="http://www.owl-ontologies.com/Movie.owl#hasScript"/>
                        <owl:allValuesFrom rdf:resource="http://www.owl-ontologies.com/Movie.owl#ComedyScript"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.owl-ontologies.com/Movie.owl#Drama"/>
        <owl:disjointWith rdf:resource="http://www.owl-ontologies.com/Movie.owl#Thriller"/>
        <owl:disjointWith rdf:resource="http://www.owl-ontologies.com/Movie.owl#War"/>
    </owl:Class>
    


